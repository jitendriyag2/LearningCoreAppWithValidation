@model LearningCoreAppWithValidation.ViewModels.CenterDetailVM
<h4>Details</h4>
<div class="row">
    <div class="col-md-4">
        @if (Model != null)
    {
        <div class="form-group">
            <label asp-for="Name" class="control-label"></label>
            <input asp-for="Name" class="form-control" disabled />
        </div>
        <div class="form-group">
            <label asp-for="ParentCenter" class="control-label"></label>
            <input asp-for="ParentCenter" class="form-control" disabled />
        </div>
        <div class="form-group">
            <label asp-for="CenterType" class="control-label"></label>
            <input asp-for="CenterType" class="form-control" disabled />
        </div>
        <div class="form-group">
            <label asp-for="IsActive" class="control-label"></label>
            <input asp-for="IsActive" class="form-inline" disabled />
        </div>

        }
        else
        {
            <div class="alert-danger"> No Detail found on this</div>

        }

        <div class="form-group">
            <a asp-action="Index">Back To List</a>
        </div>
    </div>
</div>